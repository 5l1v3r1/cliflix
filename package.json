{
  "name": "cliflix",
  "version": "1.10.0",
  "description": "Watch anything instantaneously, just write its name.",
  "bin": "dist/bin/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck",
    "compile:watch": "tsc --skipLibCheck --watch",
    "lint": "tslint \"src/**/*.ts*\" --force",
    "prepublish": "npm run clean && npm run lint && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/cliflix/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/cliflix.git"
  },
  "keywords": [
    "watch",
    "stream",
    "torrent",
    "cli",
    "cliflix"
  ],
  "dependencies": {
    "caporal": "^0.10.0",
    "chalk": "^2.4.1",
    "execa": "^0.8.0",
    "filesize-parser": "^1.5.0",
    "inquirer-helpers": "^1.3.2",
    "is-online": "^7.0.0",
    "json5": "^0.5.1",
    "locale-code": "^2.0.1",
    "lodash": "^4.17.11",
    "opensubtitles-api": "^4.1.0",
    "os-locale": "^3.0.1",
    "parse-torrent": "^6.1.2",
    "prettysize": "^0.1.0",
    "read-pkg-up": "^2.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "temp": "^0.8.3",
    "torrent-search-api": "^2.0.3",
    "update-notifier": "^2.5.0",
    "webtorrent-cli": "git://github.com/fabiospampinato/webtorrent-cli.git#iina-pip"
  },
  "devDependencies": {
    "@types/node": "^8.0.26",
    "rimraf": "^2.6.2",
    "tslint": "^5.7.0",
    "typescript": "~2.5.2"
  },
  "engines": {
    "node": ">= 6.0.0"
  }
}
